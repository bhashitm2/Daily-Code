class Solution {
public:
    string findDifferentBinaryString(vector<string>& nums) {
        string str="";
        for(int i=0;i<nums.size();i++){
            char curr=nums[i][i];
            if(curr=='1')str+='0';
            else{
                str+='1';
            }
        }
        return str;
    }
};
